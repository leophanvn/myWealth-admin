{
  "name": "mywealth-admin",
  "version": "1.0.0",
  "main": "index.js",
  "description": "MyWealth.AI system for Admin",
  "repository": {
    "type": "git",
    "url": "git@github.com:tientran0019/boilerplate-nextjs-antd-less.git"
  },
  "author": "Tien Tran <tientran0019@gmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "reactjs",
    "boilerplate",
    "redux",
    "nextjs",
    "antd",
    "less"
  ],
  "scripts": {
    "clean": "rimraf .next/ out/",
    "dev": "NODE_ENV=development env-cmd next",
    "export": "NODE_ENV=production env-cmd -f .env.production --fallback --silent next export",
    "build": "NODE_ENV=production env-cmd -f .env.production --fallback --silent next build",
    "build:staging": "NODE_ENV=staging env-cmd -f .env.staging --fallback --silent next build",
    "start": "NODE_ENV=production env-cmd -f .env.production --fallback --silent next start",
    "start:staging": "NODE_ENV=staging env-cmd -f .env.staging --fallback --silent next start",
    "lint": "next lint",
    "analyze": "NODE_ENV=production ANALYZE=true env-cmd -f .env.production --fallback --silent next build",
    "serve": "serve .next/analyze",
    "start:pm2": "NODE_ENV=production pm2 start npm --name \"admin\" -- start"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "4.21.1",
    "antd-img-crop": "^4.2.3",
    "axios": "^0.27.2",
    "braft-editor": "^2.3.9",
    "classnames": "^2.3.1",
    "crypto-js": "^4.1.1",
    "downloadjs": "^1.4.7",
    "framer-motion": "^6.3.11",
    "immer": "^9.0.15",
    "initials": "^3.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "next": "12.1.6",
    "next-redux-wrapper": "^7.0.5",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.8.1",
    "query-string": "^7.1.1",
    "rc-picker": "^2.6.9",
    "react": "^18.1.0",
    "react-confirm": "^0.2.3",
    "react-cookies": "^0.1.1",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.1",
    "react-icons": "^4.4.0",
    "react-medium-image-zoom": "^4.4.3",
    "react-quill": "^1.3.5",
    "react-redux": "^7.2.6",
    "react-use": "^17.4.0",
    "react-waypoint": "^10.3.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "babel-plugin-import": "^1.13.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-react-hooks": "^4.5.0",
    "less-vars-to-js": "^1.3.0",
    "next-plugin-antd-less": "^1.8.0",
    "webpack": "^5.73.0"
  }
}
